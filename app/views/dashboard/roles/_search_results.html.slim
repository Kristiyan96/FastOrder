- if @roles.blank?
  p.text-center
    = t "search.no-results"
- else
  table#roles.table.table-striped
    thead
      tr
        th = t 'dashboard.title'
        th = t 'dashboard.actions'
    tbody
      = render partial: "role", collection: @roles, as: :role
  = paginate @roles, theme: 'twitter-bootstrap-3'
